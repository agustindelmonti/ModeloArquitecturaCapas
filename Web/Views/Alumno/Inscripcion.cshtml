@model IEnumerable<Entities.Curso>

@{
    ViewBag.Title = "Inscripcion";
}

<h2>Inscripcion</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Materia.Descripcion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Comision)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Materia.Descripcion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Comision)
        </td>
        @*<td>
            @Html.ActionLink("Inscribirse", "Edit", new { id = item.CursoID })
        </td>*@
    </tr>
    }
</table>